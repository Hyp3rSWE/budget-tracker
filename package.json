{
  "name": "budget-tracker",
  "version": "1.0.0",
  "main": "main.js",
  "keywords": [
    "electron",
    "budget",
    "tracker"
  ],
  "author": "",
  "license": "ISC",
  "description": "A budget tracker app built with Electron",
  "devDependencies": {
    "electron": "^33.2.0",
    "electron-builder": "^25.1.8",
    "electron-rebuild": "^3.2.9"
  },
  "dependencies": {
    "pdfkit": "^0.12.3",
    "sqlite3": "^5.1.7"
  },
  "scripts": {
    "build": "electron-builder",
    "start": "electron .",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "build": {
    "appId": "com.example.budgettracker",
    "files": [
      "*.js",
      "node_modules/**/*",
      "*.json",
      "index.html",
      "styles/"    ],
    "extraResources": [
  {
    "from": "node_modules/sqlite3/lib/sqlite3-binding",
    "to": "resources/sqlite3"
  },
  {
    "from": "budget.db",
    "to": "budget.db"
  }
],
    "directories": {
      "output": "dist"
    }
  }
}
